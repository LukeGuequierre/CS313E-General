def c(nums):
    result = []
    temp = []
    c_helper(nums, result, temp)
    return result
    
def c_helper(nums, result, temp):
    if len(temp) == len(nums):
        result.append(temp[:])
        return
        
    for j in range(len(nums)):
        if nums[j] not in temp:
            temp.append(nums[j])
            c_helper(nums, result, temp)
            temp.pop(len(temp) - 1)
            
output = c([1, 2, 3, 4])
print(len(output))